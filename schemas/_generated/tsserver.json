{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Setting of tsserver",
  "properties": {
    "typescript.experimental.useTsgo": {
      "default": false,
      "markdownDescription": "%typescript.useTsgo%",
      "order": 2,
      "scope": "window",
      "tags": [
        "experimental"
      ],
      "type": "boolean"
    },
    "typescript.locale": {
      "default": "auto",
      "enum": [
        "auto",
        "de",
        "es",
        "en",
        "fr",
        "it",
        "ja",
        "ko",
        "ru",
        "zh-CN",
        "zh-TW"
      ],
      "enumDescriptions": [
        "%typescript.locale.auto%",
        "Deutsch",
        "español",
        "English",
        "français",
        "italiano",
        "日本語",
        "한국어",
        "русский",
        "中文(简体)",
        "中文(繁體)"
      ],
      "markdownDescription": "%typescript.locale%",
      "order": 3,
      "scope": "window",
      "type": "string"
    },
    "typescript.tsc.autoDetect": {
      "default": "on",
      "description": "%typescript.tsc.autoDetect%",
      "enum": [
        "on",
        "off",
        "build",
        "watch"
      ],
      "markdownEnumDescriptions": [
        "%typescript.tsc.autoDetect.on%",
        "%typescript.tsc.autoDetect.off%",
        "%typescript.tsc.autoDetect.build%",
        "%typescript.tsc.autoDetect.watch%"
      ],
      "order": 4,
      "scope": "window",
      "type": "string"
    },
    "typescript.tsdk": {
      "markdownDescription": "%typescript.tsdk.desc%",
      "order": 1,
      "scope": "window",
      "type": "string"
    }
  }
}
